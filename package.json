{
  "name": "next12-starter-template",
  "author": "Hamza Yousfi",
  "version": "1.0.0",
  "description": "A Starter Template for `production-ready` applications that want to use Nextjs Version 12 with typescript",
  "scripts": {
    "prepare": "husky install",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "test": "jest --watch",
    "test:ci": "jest --coverage",
    "cypress": "cypress open",
    "cypress:ci": "cypress run --headless",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "next": "12.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.59.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "18.14.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/supertest": "^2.0.12",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "cypress": "^12.7.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "node-mocks-http": "^1.12.1",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.4",
    "sass-loader": "^13.2.0",
    "storybook-addon-next": "^1.7.3",
    "storybook-css-modules-preset": "^1.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "license": "UNLICENSED"
}
